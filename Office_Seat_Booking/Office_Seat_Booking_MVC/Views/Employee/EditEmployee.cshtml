@model Office_Seat_Booking_Entity.Employee
@{
    ViewData["Title"] = "EditEmployee";
}
<form method="post" enctype="multipart/form-data">
    @*<h2>Admin Entry</h2><hr />*@
    <div class=" =form-group">
        <label>Id</label>
        <input type="text" asp-for="EmpID" class="form-control" placeholder="enter employee id" required />
    </div>
    <div class=" =form-group">
        <label>Employee Name</label>
        <input type="text" asp-for="Name" class="form-control" placeholder="enter employee name" required />
    </div>
    <div class=" =form-group">
        <label>Phone number</label>
        <input type="text" asp-for="PhoneNo" class="form-control" placeholder="enter phone number" required />
    </div>
    <div class=" =form-group">
        <label>Email id</label>
        <input type="text" asp-for="Email" class="form-control" placeholder="enter Email id" required />
    </div>
    <div class=" =form-group">
        <label>password</label>
        <input type="text" asp-for="Password" class="form-control" placeholder="enter movie Timing" required />
    </div>
    <div class=" =form-group">
        <label>gender</label>
        <input type="text" asp-for="Gender" class="form-control" placeholder="enter movie Timing" required />
    </div>
    <div class=" =form-group">
        <label>secret key</label>
        <input type="text" asp-for="Secret_Key" class="form-control" placeholder="enter movie Timing" required />
    </div>
    <div class=" =form-group">
        <label>role</label>
        <input type="text" asp-for="Role" class="form-control" placeholder="enter movie Timing" required />
    </div>
    <div class=" =form-group">
        <label>place</label>
        <input type="text" asp-for="Place" class="form-control" placeholder="enter movie Timing" required />
    </div>
    <div class=" =form-group">
        <label>designation</label>
        <input type="text" asp-for="Designation" class="form-control" placeholder="enter movie Timing" required />
    </div>
    <input type="submit" value="Save" class="btn btn-outline-success" />
    <input type="button" value="Reset" class="btn btn-outline-secondary" />
    <hr />
</form>
@if (ViewBag.status == "Ok")
{
    <div class="alert alert-success">
        @ViewBag.message
    </div>

}
else if (ViewBag.status == "Error")
{
    <div class="alert alert-danger">
        @ViewBag.message
    </div>
}